; Posy's Cursor Black for 125 and 175 percent
; michieldb.nl

[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

[Scheme.Cur]
Posy Black for 125 and 175 default.cur
Posy Black for 125 and 175 help.cur
Posy Black for 125 and 175 background max 96.ani
Posy for 125 and 175 wait max 96.ani
Posy for 125 and 175 beam.cur
Posy for 125 and 175 forbidden.cur
Posy Black for 125 and 175 size NS.cur
Posy Black for 125 and 175 size EW.cur
Posy Black for 125 and 175 size NwSe.cur
Posy Black for 125 and 175 size NeSw.cur
Posy for 125 and 175 move.cur
Posy for 125 and 175 hand.cur
Posy precise.cur
Posy Black pen.cur
Posy alt.cur

[Scheme.Txt]

[Strings]
CUR_DIR       = "Cursors\Posys Cursor Black for 125 and 175 percent scaling"
SCHEME_NAME   = "Posy's Cursor Black for 125 and 175 percent scaling"
pointer       = "Posy Black for 125 and 175 default.cur"
help          = "Posy Black for 125 and 175 help.cur"
work          = "Posy Black for 125 and 175 background max 96.ani"
busy          = "Posy for 125 and 175 wait max 96.ani"
text          = "Posy for 125 and 175 beam.cur"
unavailiable  = "Posy for 125 and 175 forbidden.cur"
vert          = "Posy Black for 125 and 175 size NS.cur"
horz          = "Posy Black for 125 and 175 size EW.cur"
dgn1          = "Posy Black for 125 and 175 size NwSe.cur"
dgn2          = "Posy Black for 125 and 175 size NeSw.cur"
move          = "Posy for 125 and 175 move.cur"
link          = "Posy for 125 and 175 hand.cur"
cross         = "Posy precise.cur"
hand          = "Posy Black pen.cur"
alternate     = "Posy alt.cur"